{
  "name": "devtrackr",
  "version": "1.0.0",
  "description": "Production-grade npm package for fetching and normalizing GitHub developer data",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test && npm run type-check"
  },
  "keywords": [
    "github",
    "developer",
    "analytics",
    "sdk",
    "github-api",
    "developer-stats",
    "contributions",
    "repositories",
    "commits",
    "typescript"
  ],
  "author": {
    "name": "DevTrackr Contributors"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raghaverma/Devtrackrnpm.git"
  },
  "homepage": "https://github.com/Raghaverma/Devtrackrnpm#readme",
  "bugs": {
    "url": "https://github.com/Raghaverma/Devtrackrnpm/issues"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {}
}

